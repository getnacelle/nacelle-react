{
  "npmClient": "npm",
  "packages": ["packages/*", "examples/*"],
  "version": "2.10.1",
  "publish": {
    "allowBranch": "main",
    "ignoreChanges": [
      "*.md",
      ".gitignore",
      "**/**/**.test.*",
      "**/.storybook/**",
      "**/**/*.fixtures.**"
    ],
    "message": "chore(release): publish"
  },
  "command": {
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    }
  }
}
