{
  "name": "@nacelle/react-components",
  "version": "6.0.1",
  "description": "A collection of atomic components for building Nacelle applications",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "precommit": "lint-staged",
    "build": "rollup -c",
    "test": "jest ./src --watch",
    "test:ci": "jest ./src --runInBand --coverage",
    "lint": "eslint ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "nacelle",
    "components",
    "nextjs",
    "gatsby",
    "jamstack"
  ],
  "author": "Nacelle Dev Team",
  "license": "ISC",
  "peerDependencies": {
    "@emotion/core": ">=10.0.34",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "devDependencies": {
    "@storybook/react": "^6.0.5",
    "eslint": "^7.6.0",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getnacelle/nacelle-react.git"
  },
  "bugs": {
    "url": "https://github.com/getnacelle/nacelle-react/issues"
  },
  "lint-staged": {
    "src/**/*.(js|ts|tsx)": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  },
  "gitHead": "3153c14f529fa9f691e7cd2e6c44cee74d1a78db",
  "publishConfig": {
    "access": "public"
  }
